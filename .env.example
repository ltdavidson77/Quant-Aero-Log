# Application
APP_NAME=quantaerolog
APP_ENV=development
DEBUG=true
LOG_LEVEL=debug

# Server
HOST=0.0.0.0
PORT=8000
WORKERS=4
TIMEOUT=120

# Database
DB_HOST=db
DB_PORT=5432
DB_NAME=quantaerolog
DB_USER=postgres
DB_PASSWORD=postgres
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20
DB_TIMEOUT=60

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
REDIS_SSL=false

# Monitoring
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
METRICS_PORT=8000
ENABLE_METRICS=true

# Security
SECRET_KEY=your-secret-key-here
JWT_SECRET=your-jwt-secret-here
JWT_EXPIRATION=3600
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# API Keys
YFINANCE_API_KEY=your-api-key-here

# Cache
CACHE_TYPE=redis
CACHE_REDIS_HOST=redis
CACHE_REDIS_PORT=6379
CACHE_DEFAULT_TIMEOUT=300

# Quantum Computing
QUANTUM_BACKEND=simulator
QUANTUM_SHOTS=1000
QUANTUM_OPTIMIZATION_LEVEL=1

# Model Configuration
MODEL_TYPE=xgboost
MODEL_VERSION=1.0.0
MODEL_SAVE_PATH=/app/models
FEATURE_STORE_PATH=/app/features

# Performance
BATCH_SIZE=64
NUM_WORKERS=4
PREFETCH_FACTOR=2
PIN_MEMORY=true

# Logging
LOG_FORMAT=json
LOG_PATH=/app/logs
ENABLE_FILE_LOGGING=true
ROTATION_SIZE=10MB
ROTATION_COUNT=5

# Development
ENABLE_DEBUG_TOOLBAR=false
PROFILE_QUERIES=false
EXPLAIN_QUERIES=false

# Testing
TEST_DB_NAME=test_quantaerolog
MOCK_EXTERNAL_SERVICES=true
PYTEST_TIMEOUT=300

# Docker
DOCKER_REGISTRY=your-registry.com
DOCKER_IMAGE_PREFIX=quantaerolog
DOCKER_TAG=latest 